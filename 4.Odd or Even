package FLIPTOP;

public class SidAndWyndel3 {
    public static void main(String[] args) {
        System.out.println("Odd\tEven");
        int Gapongli []={3, 9, 15, 21, 23, 21, 20, 18, 4, 14, 12, 10};
    
        for(int counter=0;counter<Gapongli.length;counter++) {
            System.out.println(Gapongli[counter]+" " );
    

        }   
     
        int odd[] = new int [6];
        int counterodd = 0;
        
        for(int counter = 0; counter<Gapongli.length;counter ++){
            if(Gapongli[counter]%2==1){
                odd [counterodd]= Gapongli[counter];
                counterodd++;
           
            }
        }
        int even[] = new int [6];
        int countereven = 0;
        
        for(int counter = 0; counter<Gapongli.length;counter ++){
            if(Gapongli[counter]%2==0){
                even [countereven]= Gapongli [counter];
                countereven++;;
             
        }        
        }
            }  
    }
